{
  "name": "nodejs-docker",
  "description": "This repository contains the Dockerfile used to build the Node.js runtime image used by Google App Engine",
  "scripts": {
    "test": "mocha -r source-map-support/register build/test/**/*.js --reporter spec --timeout 240000",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "@types/node-uuid": "0.0.28",
    "@types/request": "0.0.45",
    "gts": "latest",
    "mocha": "^2.4.5",
    "node-uuid": "^1.4.7",
    "request": "^2.69.0",
    "source-map-support": "^0.5.0",
    "tslint": "^5.2.0",
    "typescript": "^2.6.1"
  }
}
